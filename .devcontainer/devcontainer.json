{
  "name": "Odoo web container",
  "dockerComposeFile": "../docker-compose.yaml",
  "service": "web",
  "workspaceFolder": "/mnt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "dbaeumer.vscode-eslint",
        "mechatroner.rainbow-csv",
        "ms-vscode.makefile-tools",
        "GitHub.copilot"
      ]
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "echo 'Container ready!'",
  "shutdownAction": "stopCompose"
}
